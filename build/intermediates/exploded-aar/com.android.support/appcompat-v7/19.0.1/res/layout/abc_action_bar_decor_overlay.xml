<android.support.v7.internal.widget.ActionBarOverlayLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/action_bar_overlay_layout" android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <FrameLayout android:id="@id/action_bar_activity_content" android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
    <LinearLayout android:id="@+id/top_action_bar" android:layout_width="fill_parent"
        android:layout_height="wrap_content" android:layout_gravity="top">
        <android.support.v7.internal.widget.ActionBarContainer
            android:id="@+id/action_bar_container" style="?attr/actionBarStyle"
            android:layout_width="fill_parent" android:layout_height="wrap_content"
            android:layout_alignParentTop="true" android:gravity="top">
            <android.support.v7.internal.widget.ActionBarView android:id="@+id/action_bar"
                style="?attr/actionBarStyle" android:layout_width="fill_parent"
                android:layout_height="wrap_content" />
            <android.support.v7.internal.widget.ActionBarContextView
                android:id="@+id/action_context_bar" style="?attr/actionModeStyle"
                android:layout_width="fill_parent" android:layout_height="wrap_content"
                android:visibility="gone" />
        </android.support.v7.internal.widget.ActionBarContainer>
        <ImageView android:layout_width="fill_parent" android:layout_height="wrap_content"
            android:scaleType="fitXY" android:src="?android:attr/windowContentOverlay" />
    </LinearLayout>
    <android.support.v7.internal.widget.ActionBarContainer android:id="@+id/split_action_bar"
        style="?attr/actionBarSplitStyle" android:layout_width="fill_parent"
        android:layout_height="wrap_content" android:layout_gravity="bottom"
        android:gravity="center" android:visibility="gone" />
</android.support.v7.internal.widget.ActionBarOverlayLayout><!-- From: file:/Volumes/Android/int/klp-dev/frameworks/support/v7/appcompat/res/layout/abc_action_bar_decor_overlay.xml -->